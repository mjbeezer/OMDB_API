@{
}

@foreach (MovieModel M in Model)
     {
         <h3>Title: @M.Title</h3><br />
         <img src="@M.Poster" /><br />
         <h4>Rated: @M.Rated</h4><br />
         <h4>Runtime: @M.Runtime</h4><br />
     }